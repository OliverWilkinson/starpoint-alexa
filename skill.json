{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-GB": {
          "summary": "Alexa Skill for Your Radio Station",
          "examplePhrases": [
            "Alexa, open hippynet radio",
            "Alexa, play hippynet radio",
            "Alexa, ask hippynet radio to play"
          ],
          "keywords": [
            "audio streaming",
            "music",
            "radio"
          ],
          "name": "Hippynet Radio",
          "description": "An Alexa Skill for Hippynet Radio",
          "smallIconUri": "https://s3.amazonaws.com/cdn.dabblelab.com/img/skill-template-default-sm.png",
          "largeIconUri": "https://s3.amazonaws.com/cdn.dabblelab.com/img/skill-template-default-lg.png"
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "No special instructions",
      "category": "STREAMING_SERVICE",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom"
        },
        "interfaces": [
          {
            "type": "AUDIO_PLAYER"
          }
        ]
      }
    },
    "manifestVersion": "1.0",
    "permissions": [],
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "http://hippynet.co.uk/terms",
          "privacyPolicyUrl": "http://hippynet.co.uk/privacy"
        }
      },
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    }
  }
}
