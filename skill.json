{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-GB": {
          "summary": "A Starpoint radio Alexa Skill",
          "examplePhrases": [
            "Alexa, open starpoint radio",
            "Alexa, play starpoint radio",
            "Alexa, ask starpoint radio to play"
          ],
          "keywords": [
            "audio streaming",
            "music",
            "radio"
          ],
          "name": "Starpoint Radio",
          "description": "An Alexa Skill for Starpoint Radio",
          "smallIconUri": "https://s3.eu-central-1.wasabisys.com/autopod-public/hippynet-alexa/starpoint-small.png",
          "largeIconUri": "https://s3.eu-central-1.wasabisys.com/autopod-public/hippynet-alexa/starpoint-512.png"
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "No special instructions",
      "category": "STREAMING_SERVICE",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom"
        },
        "interfaces": [
          {
            "type": "AUDIO_PLAYER"
          }
        ]
      }
    },
    "manifestVersion": "1.0",
    "permissions": [],
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "https://www.starpointradio.com/",
          "privacyPolicyUrl": "https://www.starpointradio.com/"
        }
      },
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    }
  }
}
